{
  "name": "Invader",
  "version": "0.0.1",
  "description": "Tool for playing flash invader directly in Google street view.",
  "browser_action": {
    "default_icon": "icon/toolbar_2.png"
  },
  "icons": {
    "16": "icon/icon16_2.png",
    "32": "icon/icon32_2.png",
    "48": "icon/icon48_2.png",
    "128": "icon/icon128_2.png"
  },
  "manifest_version": 2,
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": ["activeTab", "tabs", "<all_urls>"]
}
